<odoo>
    <data noupdate="1">
 
        <!-- vehicle brand sequences -->
 
        <record id="seq_vehicle_brand" model="ir.sequence">
            <field name="name">Brand Sequence</field>
            <field name="code">vehicle.brand.sequence</field>
            <field name="prefix">VB</field>
            <field name="padding">3</field>
        </record>
        
        <!-- vehicle type sequences -->

        <record id="seq_vehicle_type" model="ir.sequence">
            <field name="name">Type Sequence</field>
            <field name="code">vehicle.type.sequence</field>
            <field name="prefix">VT</field>
            <field name="padding">3</field>
        </record>

        <!-- vehicle model sequences -->

        <record id="seq_vehicle_model" model="ir.sequence">
            <field name="name">Model Sequence</field>
            <field name="code">vehicle.model.sequence</field>
            <field name="prefix">VM</field>
            <field name="padding">3</field>
        </record>

        <!-- price list sequences -->

        <record id="seq_vehicle_price" model="ir.sequence">
            <field name="name">Price Sequence</field>
            <field name="code">vehicle.price.sequence</field>
            <field name="prefix">VP</field>
            <field name="padding">3</field>
        </record>
 
    </data>
</odoo>